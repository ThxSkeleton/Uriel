        </frameworkAssemblies>
        <dependencies>
            <group>
                <dependency id="OpenGL.Net" version="$version$" />
            </group>
            <group targetFramework="net35"/>
            <group targetFramework="net461"/>
        </dependencies>
    </metadata>
